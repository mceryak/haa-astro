---

<Layout activePage="Carports">
  <BackgroundImage image="plaster1">
    <div class="bg-white/40 py-10">
      <section
        class="mx-2 md:mx-10 flex flex-col items-center text-center gap-5"
      >
        <div class="flex flex-col items-center gap-2">
          <SectionTitle>Carports & Garages</SectionTitle>
        </div>
        <img src="/images/eagleCarportsLogo.webp" class="w-30 translate-x-2" />
        <p class="max-w-[55rem]">
          We proudly partner with Eagle Carports to deliver exceptional carports
          and metal garages to our customers. Check out the current or past
          featured carports, or click below to customize your own!
        </p>

        <ExternalLink href="https://build.eaglecarports.com/?dealer=D0012000">
          <img
            src="/images/buildYourOwnCarport.png"
            class="w-50"
            slot="image"
          />
          Build Your Own Carport
        </ExternalLink>
      </section>
    </div>
  </BackgroundImage>
  <Section id="featured carport" topShadow>
    <SectionTitle>Featured Design</SectionTitle>
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-5">
      <div
        class="flex flex-col gap-3 items-center md:mx-10 lg:mx-0 lg:max-w-auto"
      >
        <img src={featuredCarport.imageR2} class="rounded-sm shadow-lg" />
        <div class="hidden lg:block w-full">
          <CTARow>
            <CTA href="/" fullWidth isExternalLink>Customize This Design</CTA>
          </CTARow>
        </div>
      </div>
      <div class="md:mx-10 lg:mx-0">
        <!-- <h2 class="text-lg font-semibold lg:text-left">{featuredCarport.name}</h2>
        <div class="flex gap-2 items-center justify-center w-full -translate-x-2 lg:-translate-x-0 text-base mb-5">
          <span class="w-4 text-amber-700"><Icon name="mdi:ruler-square" /></span>
          {featuredCarport.dimensions}
        </div> -->
        <h2 class="font-semibold text-center lg:text-left mb-2">
          {featuredCarport.name}
        </h2>
        <ul
          class="text-base flex flex-col md:grid grid-cols-2 lg:grid-cols-1 items-center gap-2 md:text-left mb-4 w-full"
        >
          {
            featuredCarport.features.map((ft) => (
              <li class="flex gap-2 items-baseline ">
                <span class="w-4 text-amber-500">
                  <Icon name="mdi:star" />
                </span>
                {ft}
              </li>
            ))
          }
        </ul>
        <div class="lg:hidden w-full">
          <CTARow>
            <CTA href="/" fullWidth isExternalLink>Customize This Design</CTA>
          </CTARow>
        </div>
        <!-- <a
          href="/"
          class="mt-2 flex gap-2 items-center justify-center cursor-pointer text-zinc-200 bg-sky-800 rounded-lg p-3"
        >
          Customize This Carport
          <Icon name="mdi:launch" />
        </a> -->
      </div>
    </div>
  </Section>
  <BackgroundImage image="plaster1">
    <div class="bg-gradient-to-b from-white/70 to-red-300/20">
      <Section id="past featured carports">
        <SectionTitle>Past Featured</SectionTitle>
        <ul
          class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-5 items-center mx-2 md:mx-10"
        >
          {
            otherCarports.map((cp) => (
              <li class="relative max-w-100">
                <a class="cursor-pointer  group w-full text-white" href="/">
                  <img src={cp.thumbnailR2} class="rounded-sm shadow-lg " />
                  <div class="hidden group-focus:flex group-hover:flex gap-2 items-center justify-center absolute top-0 left-0 w-full h-full bg-black/50 rounded-sm">
                    <span>Customize</span>
                    <Icon name="mdi:launch" />
                  </div>
                </a>
              </li>
            ))
          }
        </ul>
      </Section>
    </div>
  </BackgroundImage>
  <BackgroundImage image="plaster1">
    <section id="call to action" class="bg-white/50 w-full p-5 md:p-10">
      <div
        class="flex flex-col md:flex-row gap-5 md:gap-10 w-full items-center justify-center"
      >
        <CTA isExternalLink color="blue" href="/">Customize Your Own</CTA>
      </div>
    </section>
  </BackgroundImage>
</Layout>
